{
  "name": "vue-fusion",
  "version": "0.0.1",
  "description": "vue SSR/OTA/HMR",
  "main": "publish/lib/src/index.js",
  "module": "publish/esm/src/index.js",
  "types": "publish/esm/src/index.d.ts",
  "files": [
    "publish/lib/src/*",
    "publish/esm/src/*"
  ],
  "dependencies": {
    "vue-fusion": "workspace:*",
    "@vue/shared": "^3.2.31",
    "vue": "^3.2.31",
    "express": "^4.17.1",
    "vite": "^2.8.6",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.5",
    "jest": "27.5.1",
    "@types/jest": "27.4.1",
    "@types/express": "^4.17.13"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:lib": "tsc --watch",
    "build:esm": "tsc --watch --module esnext --outDir publish/esm"
  }
}